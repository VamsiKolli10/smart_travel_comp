# Frontend environment variables for Smart Travel Companion
# Duplicate this file as .env and fill in the secrets before running `npm run dev`

# Base URL of the backend API (include /api)
VITE_API_URL=http://localhost:8000/api

# Firebase Web SDK configuration (from Firebase console)
VITE_FIREBASE_API_KEY=your_firebase_web_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=000000000000
VITE_FIREBASE_APP_ID=1:000000000000:web:abcdef123456
